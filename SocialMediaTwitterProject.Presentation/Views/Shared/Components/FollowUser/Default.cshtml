
@model FollowDTO

<input hidden asp-for="FollowerId" />
<input hidden asp-for="FollowingId" />

@{
    if (Model.isExist)
    {
        <button id="UnFollow" onclick="Follow(@Model.isExist.ToString().ToLower())" class="btn btn-info btn-sm">Unfollow</button>
    }
    else
    {
        <button id="Follow" onclick="Follow(@Model.isExist.ToString().ToLower())" class="btn btn-info btn-sm">Follow</button>
    }
}
